# Example environment variables for the exporter
# Copy this file to .env and fill in your values

# AWS Credentials
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_REGION=us-east-1

# S3 Bucket
WALG_S3_PREFIX=s3://your-bucket/mysql-backups

# MySQL Connection
WALG_MYSQL_DATASOURCE_NAME=root:password@tcp(mysql:3306)/mysql

# Compression Method (optional)
# Options: lz4, lzma, zstd, brotli
WALG_COMPRESSION_METHOD=brotli

# Encryption Key (optional)
# Generate with: head -c 32 /dev/urandom | base64
WALG_LIBSODIUM_KEY=

# Grafana Admin Password
GRAFANA_PASSWORD=admin
